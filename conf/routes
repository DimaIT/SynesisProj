# Home page
GET         /                    controllers.Application.index
GET         /responses           controllers.Application.responsesController
GET         /fields              controllers.FieldController.fields
GET         /fields/:id          controllers.FieldController.field(id: Long)
POST        /fields/:id          controllers.FieldController.saveField(id: Long)
DELETE      /fields/:id          controllers.FieldController.deleteField(id: Long)


# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.at(path="/public", file)

    